# F95Zone Credentials
F95_USERNAME=your_username
F95_PASSWORD=your_password

# Database Configuration
# Uses direct file path; Docker will mount /data to this location
DATABASE_URL=sqlite+aiosqlite:///data/avn_index.db

# F95Checker API Configuration
# Daily limit for API calls to third-party service
F95CHECKER_DAILY_LIMIT=1000

# Application Settings
LOG_LEVEL=INFO
